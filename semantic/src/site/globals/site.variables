/*-------------------
    Site Colors
--------------------*/

/*---  Colors  ---*/

@pageBackground: #F9FAFD;
@primaryColor: #88FFD4;
@secondaryColor: #0C5FFF;
@purple: #340C61;
@lightPurple: #340C61;
@olive: #98ee98;
@linkColor: #009FDA;
@linkHoverColor: darken(saturate(@linkColor, 20), 15, relative);


@fontName          : 'Open Sans';
@headerFont        : @fontName,Lato,-apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif;
@pageFont          : Lato,-apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif;