{
  "name": "product-makr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run semantic:build && next build",
    "start": "next start",
    "now-dev": "npm run semantic:build && next dev",
    "export": "next export",
    "semantic:build": "cd semantic && gulp build-css build-assets",
    "semantic:watch": "cd semantic && npm run semantic:build && gulp watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "fomantic-ui": "^2.8.7",
    "gray-matter": "^4.0.3",
    "next": "10.2.3",
    "postcss": "^8.3.0",
    "raw-loader": "^4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "remark": "^13.0.0",
    "semantic-ui-react": "^2.0.3",
    "strip-markdown": "^4.0.0"
  },
  "devDependencies": {
    "@types/gtag.js": "0.0.4",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "globby": "^11.0.3",
    "typescript": "^4.3.2"
  }
}
